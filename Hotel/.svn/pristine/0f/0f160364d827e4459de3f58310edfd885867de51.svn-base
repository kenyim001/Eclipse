package cn.tedu.hotel.entity;

public class User {
	private String name;//姓名
	private String gender;//性别
	private String IDNumber;//身份证号
	private String telephone;//手机号
	private String email;//邮箱
	private String orderNumber;//订单号
	private String remarks;//备注
	private int discount;//优惠折扣
	private int memberLevel;//会员等级
	private String accountNumber;//账号
	private String password;//密码
	private int consumeCount;//消费次数
	private int paymentStatus;//支付状态
	
	public User() {
		
	}
	
	public User(String name, String gender, String iDNumber, String telephone, String email, String orderNumber,
			String remarks, int discount, int memberLevel, String accountNumber, String password, int consumeCount,
			int paymentStatus) {
		this.name = name;
		this.gender = gender;
		IDNumber = iDNumber;
		this.telephone = telephone;
		this.email = email;
		this.orderNumber = orderNumber;
		this.remarks = remarks;
		this.discount = discount;
		this.memberLevel = memberLevel;
		this.accountNumber = accountNumber;
		this.password = password;
		this.consumeCount = consumeCount;
		this.paymentStatus = paymentStatus;
	}

	public String getName() {
		return name;
	}
	public void setName(String name) {
		this.name = name;
	}
	public String getGender() {
		return gender;
	}
	public void setGender(String gender) {
		this.gender = gender;
	}
	public String getIDNumber() {
		return IDNumber;
	}
	public void setIDNumber(String iDNumber) {
		IDNumber = iDNumber;
	}
	public String getTelephone() {
		return telephone;
	}
	public void setTelephone(String telephone) {
		this.telephone = telephone;
	}
	public String getEmail() {
		return email;
	}
	public void setEmail(String email) {
		this.email = email;
	}
	public String getOrderNumber() {
		return orderNumber;
	}
	public void setOrderNumber(String orderNumber) {
		this.orderNumber = orderNumber;
	}
	public String getRemarks() {
		return remarks;
	}
	public void setRemarks(String remarks) {
		this.remarks = remarks;
	}
	public int getDiscount() {
		return discount;
	}
	public void setDiscount(int discount) {
		this.discount = discount;
	}
	public int getMemberLevel() {
		return memberLevel;
	}
	public void setMemberLevel(int memberLevel) {
		this.memberLevel = memberLevel;
	}
	public String getAccountNumber() {
		return accountNumber;
	}
	public void setAccountNumber(String accountNumber) {
		this.accountNumber = accountNumber;
	}
	public String getPassword() {
		return password;
	}
	public void setPassword(String password) {
		this.password = password;
	}
	public int getConsumeCount() {
		return consumeCount;
	}
	public void setConsumeCount(int consumeCount) {
		this.consumeCount = consumeCount;
	}
	public int getPaymentStatus() {
		return paymentStatus;
	}
	public void setPaymentStatus(int paymentStatus) {
		this.paymentStatus = paymentStatus;
	}
	
	
	public String toString() {
		return "User [name=" + name + ", gender=" + gender + ", IDNumber=" + IDNumber + ", telephone=" + telephone
				+ ", email=" + email + ", orderNumber=" + orderNumber + ", remarks=" + remarks + ", discount="
				+ discount + ", memberLevel=" + memberLevel + ", accountNumber=" + accountNumber + ", password="
				+ password + ", consumeCount=" + consumeCount + ", paymentStatus=" + paymentStatus + "]";
	}
	
}
